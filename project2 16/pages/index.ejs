<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/style.css">
</head>
<body>
    <h1>INDEX</h1>
    <div class="container">
        <a href="/page1">PAGE 1</a>
        <a href="/page2">PAGE 2</a>
    </div>
    <%- include('./pages-ejs/module1.ejs')%>
    <ul>
        <% if (data.length>0) { %>
            <% data.forEach(({name})=> {%>
                <li>
                    <article>
                        <div class="info">
                            <span class="name">
                                <%= name %>
                            </span>
                        </div>
                    </article>
                </li>
                <% }) %>
                    <% } %>
    </ul>
</body>
</html>